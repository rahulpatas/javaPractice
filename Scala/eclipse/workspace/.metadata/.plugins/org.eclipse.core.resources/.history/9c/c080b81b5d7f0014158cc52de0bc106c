package week4
import week4._

object LinkedList {
 
 def nth[T] (n : Int, t: list[T]) : T =
 if(t.isEmpty ) throw new IndexOutOfBoundsException
 else if (n == 0) t.head
 else nth(n-1, t.tail)
 
 val list = new Cons(1, new Cons(2, new Cons (3, new Nil)))
    nth(2, list)
    nth(1, list)
    
        val a =list.apply(1,2)
 }
 